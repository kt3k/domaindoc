{
  "name": "domaindoc",
  "version": "3.0.1",
  "description": "Generate documentation site of domain models from .md sources",
  "main": "cli.js",
  "files": [
    "cli.js",
    "src"
  ],
  "scripts": {
    "test": "kocha test",
    "cov": "nyc npm test",
    "codecov": "npm run cov && codecov",
    "doc": "./cli.js build",
    "lint": "standard",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --single-quote",
      "standard --fix",
      "git add"
    ]
  },
  "bin": "./cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kt3k/domaindoc.git"
  },
  "keywords": [
    "domain",
    "ddd",
    "modeling",
    "model",
    "ssg"
  ],
  "author": "Yoshiya Hinosawa <stibium121@gmail.com> (https://twitter.com/kt3k)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kt3k/domaindoc/issues"
  },
  "homepage": "https://github.com/kt3k/domaindoc#readme",
  "dependencies": {
    "berber": "^1.1.1",
    "branch-pipe": "^1.0.1",
    "escape-html": "^1.0.3",
    "gulp-consolidate": "^0.2.0",
    "gulp-data": "^1.2.1",
    "gulp-front-matter": "^1.3.0",
    "gulp-markdown": "^2.0.1",
    "gulp-trimlines": "^1.0.0",
    "layout1": "^1.1.0",
    "micromatch": "^3.0.0",
    "nunjucks": "^3.0.0",
    "require-yaml": "^0.0.1",
    "vinyl-accumulate": "^1.4.1"
  },
  "devDependencies": {
    "codecov": "^3.0.2",
    "husky": "^0.14.3",
    "kocha": "^1.9.1",
    "lint-staged": "^7.2.0",
    "nyc": "^12.0.1",
    "prettier": "^1.13.5",
    "standard": "^11.0.1"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "lcov"
    ]
  }
}
