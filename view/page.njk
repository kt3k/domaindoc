<title>{{ file.frontMatter.name }}</title>
{% include viewDir + "/__head.njk" %}

<p><a href="{{ basepath }}/index.html">TOP</a>
<hr>
<h1>{{ file.frontMatter.name }}</h1>

{% if file.frontMatter.desc %}
  <p>{{ file.frontMatter.desc }}</p>
{% endif %}

{% if file.frontMatter.props %}
  <ul>
  {% for property in file.frontMatter.props %}
    <li>{{ property.name }} ({{property.type}}) - {{property.desc}}
  {% endfor %}
  </ul>
{% endif %}

{% include viewDir + "/__footer.njk" %}
